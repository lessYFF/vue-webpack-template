{
    "name": "vue-webpack-template",
    "author": "Bruce yff",
    "version": "v1.0.0",
    "private": true,
    "description": "vue template using webpack4",
    "keywords": [
        "vue",
        "eslint",
        "prettier",
        "webpack4",
        "typescript",
        "commitizen"
    ],
    "scripts": {
        "release": "standard-version",
        "start": "cross-env NODE_ENV=development node build/index",
        "prod": "cross-env NODE_ENV=production node build/index",
        "build": "cross-env NODE_ENV=production webpack build/build-server",
        "format": "prettier --write src/**/*.js src/**/*.vue"
    },
    "engines": {
        "node": ">=6.11.5"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/lessYFF/vue-webpack-template.git"
    },
    "dependencies": {
        "vue": "^2.5.21",
        "vue-router": "^3.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@babel/preset-env": "^7.2.3",
        "@babel/register": "^7.0.0",
        "@commitlint/cli": "^7.2.1",
        "@commitlint/config-conventional": "^7.1.2",
        "babel-loader": "^8.0.5",
        "clean-webpack-plugin": "^1.0.0",
        "copy-webpack-plugin": "^4.6.0",
        "cross-env": "^5.2.0",
        "css-loader": "^2.1.0",
        "cz-conventional-changelog": "^2.1.0",
        "eslint": "^5.12.0",
        "eslint-config-prettier": "^3.3.0",
        "eslint-plugin-prettier": "^3.0.1",
        "express": "^4.16.4",
        "happypack": "^5.0.1",
        "html-webpack-include-assets-plugin": "^1.0.6",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^1.3.1",
        "lint-staged": "^8.1.0",
        "mini-css-extract-plugin": "^0.5.0",
        "nodemon-webpack-plugin": "^4.0.7",
        "open-browser-webpack-plugin": "0.0.5",
        "prettier": "^1.15.3",
        "standard-version": "^4.4.0",
        "ts-loader": "^5.3.3",
        "typescript": "^3.2.2",
        "url-loader": "^1.1.2",
        "vue-loader": "^15.5.1",
        "vue-style-loader": "^4.1.2",
        "vue-template-compiler": "^2.5.21",
        "webpack": "^4.28.3",
        "webpack-bundle-analyzer": "^3.0.3",
        "webpack-cli": "^3.3.12",
        "webpack-dev-middleware": "^3.5.0",
        "webpack-dev-server": "^3.1.14",
        "webpack-merge": "^4.2.1",
        "webpack-parallel-uglify-plugin": "^1.1.0"
    },
    "lint-staged": {
        "*.{js,json,ts,css,md,vue}": [
            "prettier --write",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -e $GIT_PARAMS"
        }
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
